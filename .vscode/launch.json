    "version": "0.2.0",
    "configurations": [

        {
            "name": "Test Case 1 Auto",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/p3main",
            "args": [
                "auto",
                "424-p3-test1.txt"
            ],
            "preLaunchTask": "C/C++: make vscode_build",
             "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        
